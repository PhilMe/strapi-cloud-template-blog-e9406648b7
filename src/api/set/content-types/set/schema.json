{
  "kind": "collectionType",
  "collectionName": "sets",
  "info": {
    "singularName": "set",
    "pluralName": "sets",
    "displayName": "Set",
    "description": "COBI Sets - Hauptentit√§t"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "setNumber": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "subtitle": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "pieces": {
      "type": "integer"
    },
    "minifigures": {
      "type": "integer",
      "default": 0
    },
    "scale": {
      "type": "string"
    },
    "length": {
      "type": "decimal"
    },
    "width": {
      "type": "decimal"
    },
    "height": {
      "type": "decimal"
    },
    "dimensionUnit": {
      "type": "enumeration",
      "enum": ["cm", "mm", "inches"],
      "default": "cm"
    },
    "weight": {
      "type": "integer"
    },
    "year": {
      "type": "integer"
    },
    "releaseDate": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": ["available", "discontinued", "announced", "unknown"],
      "default": "unknown"
    },
    "msrp": {
      "type": "decimal"
    },
    "currency": {
      "type": "enumeration",
      "enum": ["EUR", "USD", "PLN"],
      "default": "EUR"
    },
    "ean": {
      "type": "string"
    },
    "ageGroup": {
      "type": "string"
    },
    "sourceUrl": {
      "type": "string"
    },
    "instructionUrl": {
      "type": "string"
    },
    "metaTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": ["images"]
    },
    "manufacturer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::manufacturer.manufacturer"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category"
    },
    "series": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::series.series"
    },
    "historicalContext": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::historical-context.historical-context"
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "affiliateLinks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::affiliate-link.affiliate-link",
      "mappedBy": "set"
    }
  }
}
